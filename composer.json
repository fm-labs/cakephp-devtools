{
    "name": "fm-labs/cakephp-devtools",
    "description": "CakePHP developer tools",
    "homepage": "http://github.com/fm-labs/cakephp-devtools",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0",
        "cakephp/migrations": "^3.0",
        "cakephp/plugin-installer": "^1.2",
        "cakephp/bake": "^2.0.3",
        "cakephp/cakephp-codesniffer": "~4.0.0",
        "cakephp/debug_kit": "^4.0",
        "gourmet/whoops": "~1.0",
        "josegonzalez/dotenv": "^3.2",
        "pdepend/pdepend": "*",
        "phing/phing": "2.*",
        "phploc/phploc": "*",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^0.12.18",
        "phpunit/phpunit": "^8",
        "psy/psysh": "@stable",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "3.*",
        "theseer/phpdox": "^0.12"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {}
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "bin": ["bin/cakedev"],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
