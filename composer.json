{
    "name": "fm-labs/cakephp-devtools",
    "description": "CakePHP developer tools",
    "homepage": "http://github.com/fm-labs/cakephp-devtools",
    "type": "project",
    "license": "Unlicense",
    "require": {
        "php": ">=8.1",
        "cakephp/bake": "^3.0.0",
        "cakephp/cakephp-codesniffer": "^5.0",
        "cakephp/debug_kit": "^5.0.0",
        "cakephp/migrations": "^4.6",
        "cakephp/repl": "@stable",
        "dereuromark/cakephp-ide-helper": "@stable",
        "josegonzalez/dotenv": "^4.0",
        "phing/phing": "^3.0",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5.5 || ^11.1.3",
        "psy/psysh": "^0.12.8",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "autoload": {
        "psr-4": {}
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {},
    "bin": ["bin/cakedev"],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
